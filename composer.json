{
  "name": "restruct/silverstripe-newsgrid",
  "description": "Manage News items from a Gridfield to prevent cluttering the sitetree",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "excludechildren",
    "sitetree",
    "gridfield",
    "news",
    "updates"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "restruct",
      "email": "mic@restruct.nl"
    },
    {
      "name": "patjnr",
      "email": "patrick@chitosystems.com"
    }
  ],
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4"
    "silverstripe/lumberjack": "^2.0.2",
    "restruct/silverstripe-softscheduler": "@stable",
    "restruct/silverstripe-admintweaks": "@stable"
  },
  "extra": {
    "expose": [
      "client/dist/css",
      "client/images"
    ],
    "installer-name": "newsgrid"
  }
}
